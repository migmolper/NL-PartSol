journal = {Materials},
keywords = {Aberfan flowslide,Fluidised behaviour,Gypsum tailings impoundment flowslide,Landslide propagation modelling,Perzyna viscoplasticity,SPH},
title = {{Modelling of fluidised geomaterials: The case of the aberfan and the Gypsum tailings impoundment flowslides}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-85019552391{\&}partnerID=MN8TOARS},
