title = {{Modelling of propagation with sph of 1966 aberfan flowslide: Special attention to the role of rheology and pore water pressure}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84960917461{\&}partnerID=40{\&}md5=14d0e07b13d2c1e14a58efd8d8332102},
