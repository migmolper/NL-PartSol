author = {Winkler, Daniel and Meister, Michael and Rezavand, Massoud and Rauch, Wolfgang},
doi = {10.1016/J.CPC.2016.11.011},
file = {:home/miguelmolinos/Documentos/BiblioMendeley/Winkler et al/Computer Physics Communications/2017 - gpuSPHASE—A shared memory caching implementation for 2D SPH using CUDA.pdf:pdf},
