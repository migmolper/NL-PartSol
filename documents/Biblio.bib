issn = {15261492},
journal = {CMES - Computer Modeling in Engineering and Sciences},
keywords = {MPM,Meshless methods,PIC,Petrov-Galerkin discretization},
number = {6},
pages = {477--495},
title = {{The generalized interpolation material point method}},
