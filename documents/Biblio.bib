title = {{D{\'{e}}veloppements de la m{\'{e}}thode des {\'{e}}l{\'{e}}ments finis avec des points d'int{\'{e}}gration Lagrangiens : applications {\`{a}} la g{\'{e}}om{\'{e}}canique}},
