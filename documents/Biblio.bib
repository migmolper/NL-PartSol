issn = {0045-7825},
journal = {Computer Methods in Applied Mechanics and Engineering},
month = {may},
pages = {41--53},
publisher = {North-Holland},
title = {{A stabilized Fractional Step, Runge–Kutta Taylor SPH algorithm for coupled problems in geomechanics}},
volume = {221-222},
year = {2012}
}
@article{Winkler2017a,
abstract = {Smoothed particle hydrodynamics (SPH) is a meshless Lagrangian method that has been successfully applied to computational fluid dynamics (CFD), solid mechanics and many other multi-physics problems. Using the method to solve transport phenomena in process engineering requires the simulation of several days to weeks of physical time. Based on the high computational demand of CFD such simulations in 3D need a computation time of years so that a reduction to a 2D domain is inevitable. In this paper gpuSPHASE, a new open-source 2D SPH solver implementation for graphics devices, is developed. It is optimized for simulations that must be executed with thousands of frames per second to be computed in reasonable time. A novel caching algorithm for Compute Unified Device Architecture (CUDA) shared memory is proposed and implemented. The software is validated and the performance is evaluated for the well established dambreak test case. 

