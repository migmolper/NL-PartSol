issn = {18737633},
journal = {Computers and Geotechnics},
keywords = {Dynamic coupled analysis,Large deformations,Material point method,Meshfree methods,River levees,Saturated soil},
pages = {199--214},
publisher = {Elsevier Ltd},
title = {{Coupling of soil deformation and pore fluid flow using material point method}},
