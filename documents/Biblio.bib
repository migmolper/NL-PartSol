file = {:home/miguelmolinos/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/B{\"{u}}rg, Lim, Brinkgreve - 2017 - Application of a Second-order Implicit Material Point Method.pdf:pdf},
