/***************************************/
/********** GENERAL PARAMETERS *********/
/***************************************/
KIND_ANALYSIS=MPM%U%2D%VerletLF
DENSITY=20
POISSON_MODULUS=0.3
ELASTIC_MODULUS=1.e5
TIME_STEP=0.00008
NUM_STEP=10000


/***************************************/
/******* SIMULATION FILES AND DIR ******/
/***************************************/
FEM_FILE_NAME=test/CasoDisco/FEM_Mesh.msh
MPM_FILE_NAME=test/CasoDisco/MPM_Mesh.msh
OUTPUT_DIR=test/CasoDisco

/***************************************/
/********* BOUNDARY CONDITIONS *********/
/***************************************/
BCC_NUM 4
BCC LABEL=TOP FIELD=V DIR={0,1} CURVE={test/CasoDisco/CurveConstat.txt}
BCC LABEL=BOTTOM FIELD=V DIR={0,1} CURVE={test/CasoDisco/CurveConstat.txt}
BCC LABEL=RIGHT FIELD=V DIR={1,0} CURVE={test/CasoDisco/CurveConstat.txt}
BCC LABEL=LEFT FIELD=V DIR={1,0} CURVE={test/CasoDisco/CurveConstat.txt}

/***************************************/
/**** INITIAL CONDITIONS FOR THE GPs ***/
/***************************************/
INIT_NUM 1
INIT_GP FIELD=V VALUE={0,-0.5} NUM_NODES=ALL_NODES

/***************************************/
/********** LOADS OVER THE GPs *********/
/***************************************/
F_LOAD_NUM 0
B_LOAD_NUM 1
B_LOAD_GP CURVE={test/CasoDisco/gravity.txt} NUM_NODES=ALL_NODES