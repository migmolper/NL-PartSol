Mesh RectangularMesh(double X0, double Y0,
		     double X1, double Y1,
		     double Dx, double Dy,
		     char * ElementType){
  Mesh OutMesh;

  if( strcmp(ElementType,"Linear") == 0 ){
    strcpy(OutMesh.TypeElem,ElementType);
    OutMesh.NumNodesElem = 4;
    OutMesh.Dimension = 2;
    OutMesh.N_ref = Q4;
    OutMesh.dNdX_ref = dQ4;
    OutMesh.NumNodesMesh = (int)(((X1-X0)/Dx + 1) *
				 ((Y1-Y0)/Dy + 1));

    
  }
  
  
  /* OutMesh.NumElemMesh; */
  /* OutMesh.NumNodesBound; */
  /* OutMesh.Coordinates; */
  /* OutMesh.Connectivity; */
  /* OutMesh.NodesBound; */
  /* OutMesh.ActiveElem; */
  
  /* OutMesh.TypeElem [20]; */
  
  /* OutMesh.NumberDOF; */
  
  
  return OutMesh;
}
