#include <stdio.h>
#include <stdlib.h>

int main(void){

  FILE * DatFile = fopen("ejemplo.dat","r");
  if( DatFile != NULL ){

    char * line;
    line = (char *)malloc(80);

    while ( fgets(line, sizeof line, DatFile) != NULL ){
      fputs(line,stdout);
      //      printf("%s\n",&line[0]);
    }
    fclose(DatFile);
    
  }
  else{
    perror("ejemplo.dat");
  }
  
  return 0;
}
