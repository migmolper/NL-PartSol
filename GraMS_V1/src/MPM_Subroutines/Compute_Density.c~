/*******************************************************/


double UpdateGaussPointDensity(double rho_n,
			       double Incr_TraceStrain){

  /* 1ยบ Density for the next step */
  double rho_n1;

  /* 2ยบ Update the density */
  rho_n1 = (double)rho_n/(1 + Incr_TraceStrain);

  /* 3ยบ Return density updated */
  return rho_n1;  
}

